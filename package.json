  "scripts": {
    "start": "next dev",
    "start:lib": "PORTAL_CORE_COMPONENTS_TARGET=lib next dev",
    "start:next-start": "next start",
    "start:next-start-lib": "PORTAL_CORE_COMPONENTS_TARGET=lib next start",
    "start:prod": "node server.mjs",
    "build": "next build",
    "build:lib": "PORTAL_CORE_COMPONENTS_TARGET=lib next build",
    "test": "npx jest --config jest.config.mjs",
    "test:clear-cache": "npx jest --clearCache",
    "test:updateSnapshots": "npx jest --config jest.config.mjs --updateSnapshot",
    "lint": "(npx eslint ./ --ext .js,.jsx,.ts,.tsx,.mjs,.mts || true) && exit 0",
    "lint:fix": "(npx eslint --fix ./ --ext .js,.jsx,.ts,.tsx,.mjs,.mts || true) && exit 0",
    "lib": "npm run lib:pre-process && npm run lib:babel-build && npm run lib:types && npm run lib:post-cleanup",
    "lib:babel-build": "NODE_ENV=production babel ./src/lib_components --config-file ./babel.config.json --out-dir lib --extensions \".js,.jsx,.ts,.tsx\" --copy-files --no-copy-ignored",
    "lib:clean": "(rm -r ./lib || true) && (rm -r ./src/lib_components/remoteAssets/* || true)",
    "lib:clean-build": "npm run lib:clean && npm run lib",
    "lib:types": "npx tsc --project tsconfig.d.json",
    "lib:pre-process": "node ./scripts/lib-gen/lib-cache-remote-assets.mjs --prettify-index --css-replace-relative-urls",
    "lib:post-cleanup": "npm run lib:post-clean-img && npm run lib:post-clean-tests && node ./scripts/lib-gen/lib-fix-worker-babel.mjs",
    "lib:post-clean-img": "rm -r ./lib/components/SiteMap/png && rm -r ./lib/components/SiteMap/svg_source && rm -r ./lib/components/DataThemeIcon/svg_source",
    "lib:post-clean-tests": "(rm -r ./lib/*/__tests__ || true) && (rm -r ./lib/*/*/__tests__ || true)",
    "checks": "npm run lib && npm run lint && npm run test && npm run build",
    "checks:docker": "npm run build:docker-image && npm run lib:docker && npm run test:docker && npm run build:docker",
    "start:docker": "docker run --rm --tty --interactive --volume=/$(pwd):/app/src --volume=/app/src/node_modules --volume=/app/src/.next --workdir=/app/src --publish=3010:3010 portal-core-components:latest-builder npm run start",
    "start:docker-prod": "docker run --rm --tty --interactive --volume=/$(pwd):/app/src --volume=/app/src/node_modules --volume=/app/src/.next --workdir=/app/src --publish=3000:3000 portal-core-components:latest-builder npm run start:prod",
    "build:docker-clean": "docker image rm portal-core-components:latest-builder",
    "build:docker-build-image": "docker buildx build --no-cache --tag portal-core-components:latest-builder --file ./build.Dockerfile .",
    "build:docker-image": "(npm run build:docker-clean || true) && npm run build:docker-build-image",
    "build:docker": "docker run --rm --tty --interactive --volume=/$(pwd):/app/src --volume=/app/src/node_modules --volume=/app/src/.next --workdir=/app/src portal-core-components:latest-builder npm run build",
    "lib:docker": "docker run --rm --tty --interactive --volume=/$(pwd):/app/src --volume=/app/src/node_modules --volume=/app/src/.next --workdir=/app/src portal-core-components:latest-builder npm run lib",
    "test:docker": "docker run --rm --tty --interactive --volume=/$(pwd):/app/src --volume=/app/src/node_modules --volume=/app/src/.next --workdir=/app/src portal-core-components:latest-builder npm run test:docker-command",
    "test:docker-update-snapshots": "docker run --rm --tty --interactive --volume=/$(pwd):/app/src --volume=/app/src/node_modules --volume=/app/src/.next --workdir=/app/src portal-core-components:latest-builder npm run test:docker-command-update-snapshots",
    "test:docker-command": "npm run lint && npm run test",
    "test:docker-command-update-snapshots": "npm run lint && npm run test:updateSnapshots"
  }
